generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model login {
  hash  String @db.VarChar(100)
  email String @unique
}

model users {
  id       Int      @id @default(autoincrement())
  username String   @db.VarChar(100)
  email    String   @unique
  entries  Int?     @default(0)
  joined   DateTime @default(now()) @db.Timestamp(6)
}
